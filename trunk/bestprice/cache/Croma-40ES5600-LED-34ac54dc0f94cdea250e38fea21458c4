    <script type="text/javascript">
        $(document).ready(function () { attachThumbEvent(); });

        function attachThumbEvent() {
            $('a[id^=aThumb]').click(function () {
                $.cookie('search-thumb-view', 'thumb_view');
                $('a[id^=aThumb]').fadeTo('slow', 1.0); $('a[id*=aGrid]').fadeTo('slow', 0.25);
                $("ul.display").fadeOut("fast", function () {
                    $(this).fadeIn("fast").addClass("thumb_view");
                });
            });

            $('a[id^=aGrid]').click(function () {
                $.cookie('search-thumb-view', '');
                $('a[id^=aGrid]').fadeTo('slow', 1.0);
                $('a[id*=aThumb]').fadeTo('slow', 0.25);
                $("ul.display").fadeOut("fast", function () { $(this).fadeIn("fast").removeClass("thumb_view"); });
            });
        }
    </script>
