		function searh_solr() {
			//alert("searh_solr");
			doPaging('1', 'spApplyLoader');
		}
		
		function FilterCategory() {
			//alert("FilterCategory");
			var sCurrentIndex="1";
			var oJson = { 
				strip: 'strip', 
				test: 'test', 
				PageNumber: sCurrentIndex,
				txtSearch: $('#txtSearch').val(),
				CategoryID: $('#CategoryID').val(), 
				brand_id: ((typeof(sBrandId) != 'undefined') ? sBrandId : ''),
				rdbCategories: cntrlValues('rdbCategories'),
				Hproductids : $('#IDhiddenProduct').val()
			};
			
			if($('[name=rdbAttribute]').size() > 0) oJson.rdbAttribute = cntrlValues('rdbAttribute', '|ATTR|');
		   
			$('#spProductLoader1').show(); /* Custom.disable();*/ 
			oAjaxReq = $.post('ProductSearch.aspx', oJson, function(d) { 
				$('#myspan').html(d); 
				attachThumbEvent(); 
				$('#spProductLoader1').hide();
				Custom.init();
				toggleContainer();
			});
		}
		
		function doPaging(sCurrentIndex, sLoaderId) {
			//alert("doPaging " + sCurrentIndex + "-" + sLoaderId);
			if (typeof sCurrentIndex != "undefined") {PageChanged(sCurrentIndex)}; //FYB 090112
			var oJson = { 
				strip: 'strip', 
				test: 'test', 
				PageNumber: sCurrentIndex,
				CategoryID: $('#CategoryID').val(), 
				brand_id: ((typeof(sBrandId) != 'undefined') ? sBrandId : ''), 
				txtSearch: $('#txtSearch').val(),
				ddlSort: $('#ddlSort').val(), 
				rdbBrands: cntrlValues('rdbBrands'), 
				rdbCategories: cntrlValues('rdbCategories'), 
				min: $('#slider-range').slider('values', 0), 
				max: $('#slider-range').slider('values', 1), 
				Hproductids : $('#IDhiddenProduct').val()
			};
			if($('[name=rdbAttribute]').size() > 0) oJson.rdbAttribute = cntrlValues('rdbAttribute', '|ATTR|');
		   
			$('#spProductLoader1').show(); /* Custom.disable();*/ 
			
			oAjaxReq = $.post('ProductSearch.aspx', oJson, function(d) { 
				$('#myspan').html(d); 
				attachThumbEvent(); 
				$('#spProductLoader1').hide();
				Custom.init();
				toggleContainer();
			});
		}
		
		function cntrlValues(sCntrl, sSep) {
			var o = $('[name=' + sCntrl + ']:checked');
			var s = '';
			for(var i = 0; i < o.size(); i ++)
			{
				if(s == '') s = o[i].value;
				else s += ((typeof(sSep) != 'undefined') ? sSep : ',') + o[i].value;
			}
			return s;
		}
		
		function controlValues(sCntrl) {
			// alert(sCntrl);
			 var o = $('[name=' + sCntrl + ']:checked');
			// alert(o);
			 
			var s = '';
			
			for(var i = 0; i < o.size(); i ++) {
				if(s == '') s = o[i].value;
				else s += ',' + o[i].value;
			}
			
			return s;
		}
		
		function toggleContainer() {
			$(".toggle_container").show();
			$(".trigger").removeClass('active1').unbind('click');
			$(".trigger").addClass('active1').unbind('click');
			$(".trigger").bind('click', function() { 
				$(this).toggleClass("active1").next().slideToggle("fast");
			});
		}  
		
		function PutHtmlHiddenFieldValue(message) {
			var hidden = document.getElementById('IDhiddenProduct');
			if (hidden != null) {
				  hidden.value = message;
			}
		}
	
