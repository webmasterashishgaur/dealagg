		function cntrlValues(sCntrl, sSep) {
			var o = $('[name=' + sCntrl + ']:checked');
			var s = '';
			
			for(var i = 0; i < o.size(); i ++)
			{
				if(s == '') s = o[i].value;
				else s += ((typeof(sSep) != 'undefined') ? sSep : ',') + o[i].value;
			}
			return s;
		}
		
