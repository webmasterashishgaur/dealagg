<script>
  function check_blank()
  {
	  if(document.getElementById('q').value=='Search for Item' || document.getElementById('q').value=='')
	  {
		  alert('Please Enter A Item');
		  return false;
	  }
	  else
	  {
		  return true;
	  }
  }
